{
  "name": "@tamjid10/typedash",
  "version": "0.1.0",
  "description": "Utility library with zero dependencies",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "size": "node scripts/size-check.js",
    "analyze": "rollup -c rollup.config.analyze.mjs",
    "example:reorder": "npm run build && node examples/reorderArray.js"
  },
  "keywords": [
    "utils",
    "typescript"
  ],
  "author": "Tamjid Ahmed",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.1",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "eslint": "9.10.0",
    "jest": "29.7.0",
    "rollup": "4.25.0",
    "ts-jest": "29.1.1",
    "tslib": "2.8.1",
    "typescript": "5.6.3",
    "@rollup/plugin-terser": "0.4.4",
    "gzip-size": "7.0.0",
    "rollup-plugin-visualizer": "5.12.0"
  }
}
